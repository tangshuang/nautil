{
  "name": "nautil",
  "version": "0.40.0",
  "description": "A javascript reactive framework which is for building crossing-platform applications.",
  "main": "index.js",
  "miniprogram": "miniprogram_dist",
  "scripts": {
    "clean": "rimraf dist miniprogram_dist dom lib native ssr web-component wechat index.js",
    "build": "npm run clean && npm run build:cjs && npm run build:wechat",
    "build:cjs": "babel src --out-dir . --config-file ./.build/babel.config.js --keep-file-extension --copy-files",
    "build:wechat": "copyfiles -f src/wechat/components/dynamic/* miniprogram_dist/wechat/components/dynamic && NODE_ENV=production webpack --config .build/webpack.wechat.config.js",
    "dev": "webpack-dev-server --config .examples/app/webpack.config.js",
    "postinstall": "node .scripts/files-for-wechat.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangshuang/nautil.git"
  },
  "keywords": [
    "javascript",
    "framework",
    "react"
  ],
  "author": "tangshuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tangshuang/nautil/issues"
  },
  "homepage": "https://github.com/tangshuang/nautil#readme",
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@types/react": "^17.0.11",
    "algeb": "^1.3.4",
    "immer": "^9.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.66.4",
    "rxjs": "^7.5.1",
    "ts-fns": "^10.7.0",
    "tyshemo": "^11.21.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.14.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "ansi-html": "0.0.7",
    "ansi-regex": "^6.0.1",
    "babel-loader": "^8.1.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^4.3.0",
    "glob-parent": "^6.0.2",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "react-reconciler": "^0.26.2",
    "rimraf": "^3.0.2",
    "scheduler": "^0.20.2",
    "style-loader": "^1.2.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  }
}
